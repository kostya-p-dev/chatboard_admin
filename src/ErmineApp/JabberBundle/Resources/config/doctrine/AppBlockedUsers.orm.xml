<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ErmineApp\JabberBundle\Entity\AppBlockedUsers" table="app_blocked_users">
    <indexes>
      <index name="auth_data_user_id_fk" columns="user_id"/>
      <index name="blocked_user_id_fk" columns="blocked_user_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <many-to-one field="blockedUser" target-entity="AuthData">
      <join-columns>
        <join-column name="blocked_user_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="user" target-entity="AuthData">
      <join-columns>
        <join-column name="user_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
